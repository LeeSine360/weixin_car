.footer{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  z-index: 100;
  font-size: 36rpx;
}
.footer_container {
  display: flex;
  height: 100%;
  width: 100%;
  justify-content: space-between;
}
.footer_reset {
  display: flex;
  width: 30%;
  height: 100%;
  justify-content: center;
  background-color: #fffdf8;
  color:#2f2a42;
  align-items: center;
}
.footer_reset:active{
  background-color: #fff;
}
.footer_cal {
  display: flex;
  width: 70%;
  height: 100%;
  justify-content: center;
  align-items: center;
  color:#fff;
}
.footer_cal_bt{
  display:flex;
  width: 100%;
  height: 100%;
  justify-content: center;
  align-items: center;
}
.footer_cal:active{
  background-color: #575757;
}
.footer_cal_bg{
  background-color: #2f2a42;
}
.footer_cal_bg_no{
  background-color: #a8a8a8;
}
.footer_auth_bt{
  height: 100%;
  width: 100%;
  border: 0;
  border-radius: 0;
  background-color: #2f2a42;
  color: #fff;
  padding-top: 12rpx;
  font-size: 36rpx;
}
