@import "../../css/content_list.wxss";
@import "../../css/showModel.wxss";
.body{
  margin-top: 30rpx;
}
.container {
  padding: 0 80rpx 30rpx;
  font-size: 36rpx;
  color:yellow;
}
.footer{
  position: fixed;
  bottom: 0;
  width: 100%;
}
.footer_container {
  display: flex;
  height: 120rpx;
  width: 100%;
  justify-content: space-between;
}
.footer_reset {
  display: flex;
  width: 30%;
  height: 100%;
  justify-content: center;
  background-color: #fffdf8;
  color:#2f2a42;
  align-items: center;
}
.footer_reset:active{
  background-color: #fff;
}
.footer_reset footer_reset_item {
  background-color: #fff;
  color:#2f2a42;
}
.footer_cal {
  display: flex;
  width: 70%;
  height: 100%;
  justify-content: center;
  background-color: #2f2a42;
  align-items: center;
  color:#fff;
}
.footer_cal:active{
  background-color: #575757;
}
